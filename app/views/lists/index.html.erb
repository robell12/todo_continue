<h1>Welcome to your list organizer</h1>
<hr />

<ul>
	<%@lists.each do |list| %>
	<li>
			<%= list.title %>
			<%= link_to 'Show', list_path(list) %>
			<%= link_to 'Edit', edit_list_path(list) %>
			<%= link_to 'Delete', list_path(list), method: :delete, data: {confirm: 'You sure you want to delete that dude?'} %>
		</li>
	<% end %>
</ul>
<%= link_to 'New List', new_list_path %>